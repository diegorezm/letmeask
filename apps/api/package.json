{
  "name": "@letmeask/api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "exports": {
    "./trpc": "./src/trpc.ts",
    "./router": "./src/router.ts"
  },
  "scripts": {
    "start": "node --env-file=.env --experimental-strip-types --no-warnings  src/server.ts",
    "dev": "node --env-file=.env --experimental-strip-types --no-warnings  --watch src/server.ts",
    "build": "node  --experimental-strip-types --no-warnings  src/server.ts",
    "lint": "pnpm run -r --parallel lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node --env-file=.env --experimental-strip-types --no-warnings  ./src/db/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@trpc/server": "^11.4.3",
    "drizzle-orm": "^0.44.2",
    "eslint-plugin-drizzle": "^0.2.3",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "^5.0.1",
    "postgres": "^3.4.7",
    "zod": "^3.25.75"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@types/node": "^24.0.10",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "typescript": "^5.8.3",
    "ultracite": "5.0.32"
  }
}
